{
  "name": "n8n-nodes-actualbudget",
  "version": "0.1.4",
  "description": "Interact with your Actual Budget instance",
  "keywords": [
    "n8n-community-node-package",
    "actualbudget",
    "n8n-nodes-actualbudget"
  ],
  "license": "MIT",
  "homepage": "https://github.com/slautebach/n8n-nodes-actual#readme",
  "author": {
    "name": "slautebach"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/n8n-community-nodes/n8n-nodes-actual"
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest",
    "test:one": "jest --testNamePattern"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ActualBudgetApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ActualBudget/ActualBudget.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^7.15.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^30.2.0",
    "n8n-core": "^1.119.1",
    "nock": "^14.0.10",
    "prettier": "^3.3.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@actual-app/api": "^25.11.0"
  }
}
